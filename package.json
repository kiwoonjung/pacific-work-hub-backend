{
  "name": "pacific-work-hub-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "knex --knexfile knexfile.cjs migrate:latest",
    "migrate:rollback": "knex --knexfile knexfile.cjs migrate:rollback",
    "migrate:make": "knex --knexfile knexfile.cjs migrate:make",
    "migrate:status": "knex --knexfile knexfile.cjs migrate:status"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "pg": "^8.14.1",
    "supabase": "^2.20.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.21",
    "@types/pg": "^8.15.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}